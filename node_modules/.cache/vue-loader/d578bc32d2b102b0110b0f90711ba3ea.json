ng } from '@/services';\r\nimport store from '@/store.js';\r\n\r\nexport default {\r\n    data(){\r\n     return {\r\n       teams: [],\r\n       team1: [],\r\n       show: Boolean = true\r\n     }\r\n\r\n   },\r\n   async created(){\r\n     this.teams = await Booking.getAllBooking();\r\n    this.teams.forEach(team => {\r\n        if(team.members.length <12 ) {\r\n          this.team1.push(team);\r\n        } \r\n    })\r\n    if(this.addMember(id)){\r\n    this.show = true;\r\n    } else {\r\n        this.show = false;\r\n    }\r\n   },\r\n\r\n   methods: {\r\n       async addMember(id){\r\n           try{\r\n        let team = this.teams.find(x => x._id == id);\r\n        let user = store.name;\r\n        team.members.push(user);\r\n        await Booking.update_team(team);\r\n           }catch (e) {\r\n      }\r\n      this.show = false;\r\n   },\r\n  \r\n},\r\n components: {\r\n        navigation\r\n    }\r\n}\r\n</script>\r\n\r\n"]}]}